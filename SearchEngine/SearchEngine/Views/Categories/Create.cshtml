@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/Master.cshtml";
}
@section head{
    <script src="~/Content/Admin/javascript/validate.js"></script>
}

<main class="main-cont content mCustomScrollbar">
    <div class="page-wrap">
        <!--开始::内容-->
        <section class="page-hd">
            <header>
                <h2 class="title">添加图书类别</h2>
               
            </header>
            <hr>
        </section>
        <form id="add" method="post" action="/Categories/Create">
            <div class="form-group-col-2">
                <div class="form-label">类别：</div>
                <div class="form-cont">
                    <input type="text" name="TypeName" placeholder="类别..." class="form-control form-boxed" style="width:300px;">

                </div>
            </div>

           
            <div class="form-group-col-2">
                <div class="form-label"></div>
                <div class="form-cont">
                    <a href="#" class="btn btn-primary" onclick="check()">提交表单</a>
                </div>
            </div>
        </form>
        <!--开始::结束-->
    </div>

</main>
